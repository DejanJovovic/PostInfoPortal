{
  "expo": {
    "name": "PostInfoPortal",
    "slug": "PostInfoPortal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/favicon.png",
    "scheme": "postinfoportal",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.gosmartdevelopment.postinfoportal",
      "infoPlist": { "UIBackgroundModes": ["remote-notification"] },
      "entitlements": {
        "aps-environment": "development",
        "com.apple.security.application-groups": [
          "group.com.gosmartdevelopment.postinfoportal.onesignal"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/favicon.png"
      },
      "package": "com.gosmartdevelopment.postinfoportal",
      "permissions": [
        "POST_NOTIFICATIONS"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme": "mailto"
          },
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "data": {
            "scheme": "https",
            "host": "linkedin.com"
          },
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "data": {
            "scheme": "whatsapp"
          },
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icons/favicon.png"
    },
    "plugins": [
      [
        "onesignal-expo-plugin",
        {
          "mode": "development"
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/favicon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1e1c5dca-b0b2-4614-98c9-6a1f09d788e3"
      }
    }
  }
}
